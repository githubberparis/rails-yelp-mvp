<%= form_for restaurant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
   <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <%= f.label :category %>
  <%= f.select :category, collection: ["chinese", "italian", "japanese", "french", "belgian"] %>
  <%#= f.label :rating %>
  <%#= f.number_field :rating %>
  <%= f.submit %>
<% end %>
